<templateSet group="Google Go">
    <template resource-bundle="ro.redeul.google.go.template.TemplateBundle" key="livetemplate.description.sout"
              name="sout" toReformat="true" value="fmt.Println(&quot;$END$&quot;)">
        <context>
            <option name="GO_FUNCTION" value="true" />
        </context>
    </template>

    <template resource-bundle="ro.redeul.google.go.template.TemplateBundle" key="livetemplate.description.souf"
              name="souf" toReformat="true" value="fmt.Printf(&quot;$CONTENT$\n&quot;$END$)">
        <variable alwaysStopAt="true" defaultValue="" expression="" name="CONTENT" />
        <context>
            <option name="GO_FUNCTION" value="true" />
        </context>
    </template>

    <template resource-bundle="ro.redeul.google.go.template.TemplateBundle" key="livetemplate.description.soutm"
              name="soutm" toReformat="true" value="fmt.Println(&quot;$FUNCTION$&quot;)">
        <variable alwaysStopAt="false" defaultValue="" expression="functionName()" name="FUNCTION" />
        <context>
            <option name="GO_FUNCTION" value="true" />
        </context>
    </template>

    <template resource-bundle="ro.redeul.google.go.template.TemplateBundle" key="livetemplate.description.soutp"
              name="soutp" toReformat="true" value="fmt.Printf(&quot;$FUNCTION$($PARAMETER_FORMAT$)\n&quot;$PARAMETER_LIST$)">
        <variable alwaysStopAt="false" defaultValue="" expression="functionName()" name="FUNCTION" />
        <variable alwaysStopAt="false" defaultValue="" expression="parameterFormat(&quot;%#v&quot;)" name="PARAMETER_FORMAT" />
        <variable alwaysStopAt="false" defaultValue="" expression="parameterList(&quot;, &quot;)" name="PARAMETER_LIST" />
        <context>
            <option name="GO_FUNCTION" value="true" />
        </context>
    </template>
</templateSet>